<div class="card">
    <div class="card-header">
        <h3 class="card-title">Custom Links</h3>
    </div>
    <div class="list-group list-group-flush">
        {% for link in links %}
            <a href="{{ link.url }}" class="list-group-item list-group-item-action" target="_blank">{{ icon('custom_links_history_external') }} {{ link.linkname }}</a>
        {% endfor %}
    </div>
</div>
